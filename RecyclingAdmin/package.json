{
  "name": "recyclingadmin",
  "version": "1.0.0",
  "description": "IT设备回收后台管理系统 - 基于Vue 3 + TypeScript + Element Plus",
  "private": true,
  "type": "module",
  "keywords": [
    "vue3",
    "typescript",
    "element-plus",
    "admin",
    "recycling"
  ],
  "author": "RecyclingAdmin Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 3001",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "vue-tsc --noEmit",
    "serve": "npm run preview",
    "start": "npm run dev"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@microsoft/signalr": "^8.0.7",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "element-plus": "^2.10.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all2": "^7.0.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
